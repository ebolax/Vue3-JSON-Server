{
    "name": "vue3-json-server",
    "version": "1.0.0",
    "description": "Vue.js 3 + JSON-Server",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "build": "node esbuild.config.js prod",
        "watch": "node esbuild.config.js dev",
        "serve": "node .",
        "nodemon": "nodemon --watch dist/server.js --exec node .",
        "dev": "concurrently \"npm run nodemon\" \"npm run watch\" \"npm run livereload\"",
        "livereload": "node livereload.js"
    },
    "keywords": [
        "esbuild",
        "vue",
        "vue 3",
        "bootstrap",
        "bootstrap 5",
        "livereload",
        "typescript"
    ],
    "author": "ebolax",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/json-server": "^0.14.4",
        "@types/node": "^16.7.7",
        "@types/underscore": "^1.11.3",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "axios": "^0.21.1",
        "bootstrap": "^5.1.0",
        "concurrently": "^6.2.1",
        "esbuild": "^0.12.24",
        "esbuild-plugin-vue3": "^0.2.2",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^7.17.0",
        "express": "^4.17.1",
        "json-server": "^0.16.3",
        "livereload": "^0.9.3",
        "nodemon": "^2.0.12",
        "underscore": "^1.13.1",
        "vue": "^3.2.2",
        "vue-router": "^4.0.11"
    }
}
