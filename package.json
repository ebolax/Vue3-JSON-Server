{
    "name": "vue3-json-server",
    "version": "1.0.0",
    "description": "Vue.js 3 + JSON-Server",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "build": "node esbuild.config.js prod",
        "watch": "node esbuild.config.js dev",
        "serve": "node .",
        "nodemon": "nodemon --watch dist/server.js --exec node .",
        "dev": "concurrently \"npm run nodemon\" \"npm run watch\" \"npm run livereload\"",
        "livereload": "node livereload.js",
        "eslint": "eslint ."
    },
    "keywords": [
        "esbuild",
        "vue",
        "vue 3",
        "bootstrap",
        "bootstrap 5",
        "livereload",
        "typescript"
    ],
    "author": "ebolax",
    "license": "ISC",
    "devDependencies": {
        "@types/bootstrap": "^5.1.6",
        "@types/express": "^4.17.13",
        "@types/json-server": "^0.14.4",
        "@types/node": "^17.0.2",
        "@types/underscore": "^1.11.4",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "axios": "^0.24.0",
        "bootstrap": "^5.1.3",
        "concurrently": "^6.5.1",
        "esbuild": "^0.14.7",
        "esbuild-plugin-vue3": "^0.3.0",
        "eslint": "^8.5.0",
        "eslint-plugin-vue": "^8.2.0",
        "express": "^4.17.2",
        "json-server": "^0.17.0",
        "livereload": "^0.9.3",
        "nodemon": "^2.0.15",
        "typescript": "^4.5.4",
        "underscore": "^1.13.2",
        "vue": "^3.2.26",
        "vue-router": "^4.0.12"
    }
}